{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2021",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./tests-dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "types": ["node", "@cloudflare/workers-types"]
  },
  "include": [
    "tests/**/*.ts",
    "src/services/perfMetricsService.ts"
  ]
}
